{
    "name": "littleboy/yii2-date-picker-thai-widget",
    "description": "Bootstrap4 DatePicker Thai Buddhist-Era (B.E.) widget for Yii2.",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "yii 2",
        "extension",
        "widget",
        "datepicker",
		"calendar",
		"buddhist",
		"thai"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "littleboy",
            "email": "aekkapun.dev@hotmail.com",
            "role": "Learning Dev"
        }
    ],    
    "require": {
        "yiisoft/yii2": ">=2.0.9",
        "yiisoft/yii2-bootstrap4": "~2.0.6",
        "bower-asset/bootstrap-datepicker": "1.9.0"
    },
    "autoload": {
        "psr-4": {
            "littleboy\\datepicker\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "bower-asset-library": "vendor/bower"
        }
    }
}
